[core]
  editor = mvim -f
[gui]
[color]
	ui = auto
[advice]
	statusHints = false
[diff]
	renames = copies
[merge]
	stat = true
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  gg = grep --break --heading -n
